$('#notifications').html('<%= escape_javascript render(@notifications) %>');
$('#notifications-paginator').html('<%= escape_javascript(paginate(@notifications, :remote => true).to_s) %>');

$("table:not(.simple) tr:nth-child(odd)").addClass("odd"); // Table Odd/Even
$("table:not(.simple) tr:nth-child(even)").addClass("even"); // Table Odd/Even

$("tbody tr").hover(
   	function() {
    	$(this).addClass("highlight");
   	},
   	function() {
    	$(this).removeClass("highlight");
   	}
  );