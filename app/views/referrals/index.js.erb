$('#referrals').html('<%= escape_javascript render(@referrals) %>');
$('#referrals-paginator').html('<%= escape_javascript(paginate(@referrals, :remote => true).to_s) %>');

$("table:not(.simple) tr:nth-child(odd)").addClass("odd"); // Table Odd/Even
$("table:not(.simple) tr:nth-child(even)").addClass("even"); // Table Odd/Even